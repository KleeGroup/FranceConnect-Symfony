{
  "name": "kleegroup/franceconnect-bundle",
  "type": "symfony-bundle",
  "description": "Symfony 4, 5 & 6 OpenID security extension FranceConnect",
  "keywords": ["Security", "OpenId", "Authentication"],
  "homepage": "https://github.com/KleeGroup/FranceConnect-Symfony",
  "license": "MIT",
  "authors": [
    {
      "name": "Klee Group",
      "homepage": "http://www.kleegroup.com/"
    }
  ],
  "require": {
    "php": "^8.1.9",
    "ext-json": "*",
    "ext-curl": "*",
    "namshi/jose": "7.2.*",
    "mashape/unirest-php": "3.*",
    "symfony/config": "4.4.* || 5.4.* || >=6.0",
    "symfony/dependency-injection": "4.4.* || 5.4.* || >=6.0",
    "symfony/framework-bundle": "4.4.* || 5.4.* || >=6.0",
    "symfony/http-foundation": "4.4.* || 5.4.* || >=6.0",
    "symfony/http-kernel": "4.4.* || 5.4.* || >=6.0",
    "symfony/routing": "4.4.* || 5.4.* || >=6.0",
    "symfony/security-bundle": "4.4.* || 5.4.* || >=6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": { "KleeGroup\\FranceConnectBundle\\": "" }
  },
  "autoload-dev": {
    "psr-4": { "KleeGroup\\FranceConnectBundle\\Tests\\": "Tests/" }
  },
  "suggest": {
    "doctrine/orm": "If you want store identities to database",
    "doctrine/mongodb-odm": "If you want store identities to mongodb"
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  }
}
